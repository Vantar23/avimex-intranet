<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Next.*" stopProcessing="true">
                    <match url=".*" />
                    <action type="Redirect" url="http://avimexintranet.com.mx:3000" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
